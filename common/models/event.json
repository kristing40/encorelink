{
  "name": "Event",
  "plural": "Events",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "notes": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "endDate": {
      "type": "date"
    },
    "created": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    },
    "volunteers": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "eventId",
      "through": "EventVolunteer",
      "keyThrough": "volunteerId"
    },
    "eventvolunteers": {
      "type": "hasMany",
      "model": "EventVolunteer",
      "foreignKey": "eventId"
    }
  },
  "acls": [],
  "methods": {}
}
